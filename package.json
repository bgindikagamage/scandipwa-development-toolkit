{
	"name": "scandipwa-devtools",
	"displayName": "scandipwa-devtools",
	"description": "ScandiPWA development toolkit",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:scandipwa-devtools.createNewComponent",
		"onCommand:scandipwa-devtools.createNewRoute",
		"onCommand:scandipwa-devtools.createNewQuery",
		"onCommand:scandipwa-devtools.extendCoreQuery"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "scandipwa-devtools.createNewComponent",
				"title": "NEW Create new component"
			},
			{
				"command": "scandipwa-devtools.createNewRoute",
				"title": "NEW Create new route"
			},
			{
				"command": "scandipwa-devtools.createNewQuery",
				"title": "NEW Create new query"
			},
			{
				"command": "scandipwa-devtools.extendCoreQuery",
				"title": "NEW Extend core query"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@babel/parser": "^7.9.6",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
